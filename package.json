{
  "name": "FastFurious",
  "description": "Our team special plugin",
  "author": "ShuYuan <beyondyuanshu@gmail.com>",
  "identifier": "",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/rodi01/RenameIt"
  },
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Fast-Furious",
    "manifest": "src/manifest.json",
    "main": "Fast-Furious.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  }
}
