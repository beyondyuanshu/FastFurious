{
  "name": "FastFurious",
  "description": "Our team special plugin",
  "author": "ShuYuan <beyondyuanshu@gmail.com>",
  "identifier": "",
  "version": "1.0.3",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/beyondyuanshu/FastFurious"
  },
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Fast-Furious",
    "manifest": "src/manifest.json",
    "main": "Fast-Furious.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "format": "prettier --write '**/*.{js,jsx}'"
  },
  "devDependencies": {
    "@skpm/babel-preset": "^0.2.1",
    "@skpm/builder": "^0.7.0",
    "@skpm/extract-loader": "^2.0.2",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-root-import": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.12.4",
    "html-loader": "^0.5.1",
    "prettier": "^1.9.2",
    "svg-react-loader": "^0.4.6"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "sketch-module-user-preferences": "^1.0.2",
    "sketch-module-web-view": "^3.1.4",
    "skpm": "^1.3.0"
  }
}
